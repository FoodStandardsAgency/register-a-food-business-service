{
  "name": "register-a-food-business-service",
  "version": "3.5.0",
  "description": "",
  "main": "src/app.js",
  "engines": {
    "node": "8.9"
  },
  "scripts": {
    "test": "jest ./src --coverage",
    "test:unit:ci": "export TEST_TYPE=unit && jest ./src --coverage",
    "test:watch": "jest --watch",
    "test:integration": "export TEST_TYPE=integration && jest tests/integration/*.js",
    "test:contract": "jest tests/contract/*.js",
    "test:component": "cucumber-js",
    "test:component:ci": "mkdir -p reports && cucumber-js --format=json | cucumber-junit > reports/TEST-component.xml",
    "test:securityall": "snyk wizard",
    "test:security": "snyk wizard --severity-threshold=medium",
    "lint": "eslint ./src",
    "format:verify": "prettier --list-different '{src,tests}/**/*.js'",
    "format": "prettier --write '{src,tests}/**/*.js'",
    "start": "node src/app.js",
    "start:prod": "export NODE_ENV=production && node src/app.js",
    "seed-config-db": "node src/connectors/configDb/configDb.seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FoodStandardsAgency/register-a-food-business-service.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FoodStandardsAgency/register-a-food-business-service#issues"
  },
  "homepage": "https://github.com/FoodStandardsAgency/register-a-food-business-service#readme",
  "dependencies": {
    "@slice-and-dice/fsa-rof": "1.0.3",
    "@slice-and-dice/register-a-food-business-validation": "1.13.1",
    "body-parser": "1.18.3",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-rate-limit": "3.2.0",
    "helmet": "3.13.0",
    "jsonschema": "1.2.4",
    "moment": "2.22.2",
    "mongodb": "3.1.3",
    "node-fetch": "2.1.2",
    "notifications-node-client": "4.2.0",
    "pg": "7.4.3",
    "request": "2.87.0",
    "request-promise-native": "1.0.5",
    "sequelize": "4.38.0",
    "sequelize-cli": "^4.1.1",
    "uuid": "3.3.2",
    "validator": "10.2.0",
    "winston": "2.4.3"
  },
  "devDependencies": {
    "cucumber": "4.2.1",
    "cucumber-junit": "^1.7.1",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.1",
    "jest": "22.4.1",
    "jest-junit": "5.1.0",
    "prettier": "1.12.1",
    "snyk": "^1.93.0"
  }
}
