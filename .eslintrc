{
  "env": {
    "node": true,
    "jest": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "curly": "warn",
    "prettier/prettier": "warn",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^next" }]
  },
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "prettier"]
}
